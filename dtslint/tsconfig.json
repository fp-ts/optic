{
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noErrorTruncation": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2021",
    "lib": [
      "ES2021"
    ],
    "paths": {
      "@fp-ts/optic": [
        "../src/index.ts"
      ],
      "@fp-ts/optic/test/*": [
        "../test/*"
      ],
      "@fp-ts/optic/examples/*": [
        "../examples/*"
      ],
      "@fp-ts/optic/*": [
        "../src/*"
      ]
    }
  }
}
